{
  "manifest_version": 3,
  "name": "Roblox Server Joiner",
  "version": "1.0",
  "action": {
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
    /* NO default_popup here */
  },
  "permissions": ["tabs", "storage"],
  "background": {
    "service_worker": "background.js"
  }
}
